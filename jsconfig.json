{
  "extends": "./playground/.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "nuxt-booster/*": [
        "src/*"
      ],
      "#booster/*": [
        "src/runtime/*"
      ],
      "@/*": [
        "playground/*"
      ],
      "@@/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
