{
  "name": "nuxt-booster",
  "version": "3.3.2",
  "description": "Nuxt Booster takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "author": "Stephan Gerbeth",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basics/nuxt-booster.git"
  },
  "type": "module",
  "scripts": {
    "prepack": "nuxt prepare playground && nuxt-module-build build",
    "prepare": "husky",
    "build": "nuxt build playground",
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build prepare",
    "dev:test": "nuxt dev test",
    "generate": "nuxt generate playground",
    "preview": "nuxt preview playground",
    "lint": "nuxt prepare playground && npm run lint:css && npm run lint:es",
    "lint:es": "eslint .",
    "lint:css": "stylelint \"(src|playground)/**/*.vue\"",
    "lint:report": "nuxt prepare playground && eslint -f json-relative -o ./eslint-report.json .",
    "test": "npm run vitest:run",
    "test:unit": "vitest ./test/tests/utils",
    "test:dev": "npm run vitest:dev",
    "vitest:run": "nuxt generate test && vitest run",
    "vitest:dev": "nuxt generate test && vitest dev",
    "start": "npx serve playground/dist",
    "start:generate": "npm run generate && npx serve playground/dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "nuxt prepare playground && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/image": "2.0.0",
    "@nuxtjs/fontaine": "0.5.0",
    "browserslist-useragent-regexp": "4.1.3",
    "cheerio": "1.1.2",
    "defu": "6.1.4",
    "dom-serializer": "2.0.0",
    "dynamic-class-list": "2.0.2",
    "htmlparser2": "10.0.0",
    "image-meta": "0.2.2",
    "mime-types": "3.0.2",
    "pathe": "2.0.3",
    "sort-css-media-queries": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@nuxt/eslint": "1.10.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/webpack-builder": "4.2.1",
    "@types/finalhandler": "1.2.4",
    "@types/jsdom": "27.0.0",
    "@types/mime-types": "3.0.1",
    "@types/node": "24.10.1",
    "@types/serve-static": "2.2.0",
    "@types/vimeo": "2.1.8",
    "@types/webpack": "5.28.5",
    "@types/youtube": "0.1.2",
    "commitlint": "20.2.0",
    "consola": "3.4.2",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "3.0.1",
    "finalhandler": "2.1.1",
    "get-port-please": "3.2.0",
    "husky": "9.1.7",
    "jsdom": "27.2.0",
    "lint-staged": "16.2.7",
    "markdown-it-inline-comments": "1.0.1",
    "nuxt": "4.2.1",
    "playwright": "1.57.0",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.8.0",
    "postcss-nesting": "13.0.2",
    "postcss-preset-env": "10.5.0",
    "prettier": "3.7.4",
    "read-pkg": "10.0.0",
    "serve-static": "2.2.0",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-config-standard": "39.0.1",
    "typescript-eslint": "8.48.1",
    "vite-plugin-checker": "0.11.0",
    "vite-svg-loader": "5.1.0",
    "vitepress": "1.6.4",
    "vitest": "4.0.15",
    "vue-semantic-structure": "1.0.5",
    "vue-tsc": "3.1.5",
    "wicg-inert": "3.1.3"
  }
}
