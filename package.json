{
  "name": "nuxt-booster",
  "version": "3.1.11",
  "description": "Nuxt Booster takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "author": "Stephan Gerbeth",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basics/nuxt-booster.git"
  },
  "type": "module",
  "scripts": {
    "prepack": "nuxt prepare playground && nuxt-module-build build",
    "prepare": "husky",
    "build": "nuxt build playground",
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build prepare",
    "generate": "nuxt generate playground",
    "preview": "nuxt preview",
    "lint": "nuxt prepare playground && npm run lint:css && npm run lint:es",
    "lint:es": "eslint .",
    "lint:css": "stylelint \"(src|playground)/**/*.vue\"",
    "lint:report": "nuxt prepare playground && eslint -f json-relative -o ./eslint-report.json .",
    "test": "npm run vitest:run",
    "vitest:run": "nuxt generate test && vitest run",
    "vitest:dev": "nuxt generate test && vitest dev",
    "start": "npx serve playground/dist",
    "start:generate": "npm run generate && npx serve playground/dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/image": "1.9.0",
    "@nuxtjs/fontaine": "0.5.0",
    "browserslist-useragent-regexp": "4.1.3",
    "cheerio": "1.0.0",
    "defu": "6.1.4",
    "dom-serializer": "2.0.0",
    "dynamic-class-list": "2.0.2",
    "htmlparser2": "10.0.0",
    "image-meta": "0.2.1",
    "mime-types": "2.1.35",
    "pathe": "2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@nuxt/eslint": "1.1.0",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/webpack-builder": "3.15.4",
    "@types/consola": "2.2.8",
    "@types/finalhandler": "1.2.3",
    "@types/jsdom": "21.1.7",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.13.9",
    "@types/rimraf": "4.0.5",
    "@types/serve-static": "1.15.7",
    "@types/vimeo": "2.1.8",
    "@types/vue": "2.0.0",
    "@types/webpack": "5.28.5",
    "@types/youtube": "0.1.0",
    "commitlint": "19.7.1",
    "consola": "3.4.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-security": "3.0.1",
    "finalhandler": "1.3.1",
    "get-port-please": "3.1.2",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "markdown-it-inline-comments": "1.0.1",
    "nuxt": "3.15.4",
    "playwright": "1.50.1",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.8.0",
    "postcss-nesting": "13.0.1",
    "postcss-preset-env": "10.1.5",
    "prettier": "3.5.3",
    "read-pkg": "9.0.1",
    "serve-static": "1.16.2",
    "stylelint-config-recess-order": "6.0.0",
    "stylelint-config-standard": "37.0.0",
    "typescript-eslint": "8.26.0",
    "vite-svg-loader": "5.1.0",
    "vitepress": "1.6.3",
    "vitest": "3.0.7",
    "vue-semantic-structure": "1.0.5",
    "vue-tsc": "2.2.8",
    "wicg-inert": "3.1.3"
  }
}
