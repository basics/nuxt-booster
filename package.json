{
  "name": "nuxt-speedkit",
  "version": "3.0.0-next.46",
  "description": "Nuxt Speedkit takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-speedkit"
  },
  "type": "module",
  "scripts": {
    "prepack": "nuxt-module-build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "nuxt build playground",
    "dev": "nuxt dev playground",
    "generate": "nuxt generate playground",
    "preview": "nuxt preview",
    "lint": "npm run lint:css && npm run lint:es",
    "lint:es": "eslint --fix --ext .js,.js,.vue .",
    "lint:css": "stylelint \"(src|playground)/**/*.vue\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "npm run vitest:run",
    "vitest:run": "nuxt generate test && vitest run",
    "vitest:dev": "nuxt generate test && vitest dev",
    "start": "npx serve playground/dist",
    "start:generate": "npm run generate && npx serve playground/dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/image": "1.3.0",
    "@nuxtjs/fontaine": "0.4.1",
    "browserslist-useragent-regexp": "4.1.1",
    "cheerio": "1.0.0-rc.12",
    "defu": "6.1.4",
    "dom-serializer": "2.0.0",
    "dynamic-class-list": "2.0.2",
    "htmlparser2": "9.1.0",
    "image-meta": "0.2.0",
    "pathe": "1.1.2",
    "serialize-to-js": "3.1.2",
    "sort-css-media-queries": "2.2.0",
    "vue3-lazy-hydration": "1.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.23.10",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/webpack-builder": "3.10.1",
    "@nuxtjs/eslint-config": "12.0.0",
    "@types/node": "20.11.16",
    "commitlint": "18.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-jsonc": "2.13.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-xss": "0.1.12",
    "finalhandler": "1.2.0",
    "get-port-please": "3.1.2",
    "husky": "9.0.10",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "markdown-it-inline-comments": "1.0.1",
    "nuxt": "3.10.1",
    "pinst": "3.0.0",
    "playwright": "1.41.2",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.6.0",
    "postcss-nesting": "12.0.2",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.2.5",
    "read-pkg": "9.0.1",
    "serve-static": "1.15.0",
    "stylelint-config-recess-order": "4.5.0",
    "stylelint-config-standard": "36.0.0",
    "vite-svg-loader": "5.1.0",
    "vitepress": "1.0.0-rc.42",
    "vitest": "1.2.2",
    "wicg-inert": "3.1.2"
  }
}
