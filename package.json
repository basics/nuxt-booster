{
  "name": "nuxt-speedkit",
  "version": "2.1.3",
  "description": "Nuxt Speedkit takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-speedkit"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "nuxt example",
    "build": "nuxt build --config-file example/nuxt.config.js",
    "generate": "nuxt generate --config-file example/nuxt.config.js --target static",
    "lint": "npm run lint:css && npm run lint:es",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:css": "stylelint \"(src|lib)/**/*.vue\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "npm run lint && npm run vitest:run",
    "vitest:run": "vitest run",
    "vitest:dev": "vitest dev",
    "start": "nuxt start --config-file example/nuxt.config.js --target static",
    "start:generate": "npm run generate && npm run start"
  },
  "dependencies": {
    "@nuxt/image": "0.7.1",
    "bowser": "2.11.0",
    "browserslist-useragent-regexp": "3.0.2",
    "buffer-image-size": "0.6.4",
    "deepmerge": "4.2.2",
    "defu": "6.1.2",
    "dynamic-class-list": "2.0.2",
    "native-fetch": "4.0.2",
    "serialize-to-js": "3.1.2",
    "sort-css-media-queries": "2.1.0",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxt/postcss8": "1.1.3",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "commitlint": "17.4.2",
    "core-js": "3.27.2",
    "eslint": "8.32.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-security": "1.6.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-xss": "0.1.12",
    "finalhandler": "1.2.0",
    "get-port-please": "3.0.1",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.0",
    "nuxt": "2.15.8",
    "pinst": "3.0.0",
    "playwright": "1.30.0",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.5.0",
    "postcss-nesting": "10.2.0",
    "postcss-preset-env": "8.0.0",
    "serve-static": "1.15.0",
    "stylelint": "14.16.1",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-standard": "29.0.0",
    "vitest": "0.28.2",
    "vue-headings": "1.0.0",
    "vue-svg-loader": "0.16.0",
    "webpack-bundle-analyzer": "4.7.0",
    "wicg-inert": "3.1.2"
  }
}
