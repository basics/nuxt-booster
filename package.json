{
  "name": "nuxt-speedkit",
  "version": "2.1.5-beta.2",
  "description": "Nuxt Speedkit takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-speedkit"
  },
  "scripts": {
    "prepare": "esbuild prepare/supportedBrowserDetector.mjs --platform=node --bundle --outfile=prepare/dist/supportedBrowserDetector.mjs --external:browserslist --external:argue-cli --external:easy-table --external:picocolors",
    "prepack": "nuxt-module-build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "npm run prepare && nuxt playground",
    "build": "npm run prepare && nuxt build --config-file playground/nuxt.config.js",
    "generate": "npm run prepare && nuxt generate --config-file playground/nuxt.config.js --target static",
    "lint": "npm run lint:css && npm run lint:es",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:css": "stylelint \"(src|playground)/**/*.vue\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "npm run prepare && npm run lint && npm run vitest:run",
    "vitest:run": "vitest run",
    "vitest:dev": "vitest dev",
    "start": "nuxt start --config-file playground/nuxt.config.js --target static",
    "start:generate": "npm run generate && npm run start"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/image": "0.7.1",
    "@nuxt/kit": "^3.4.1",
    "@nuxt/module-builder": "^0.3.0",
    "bowser": "2.11.0",
    "browserslist-useragent-regexp": "4.0.0",
    "buffer-image-size": "0.6.4",
    "deepmerge": "4.3.1",
    "defu": "6.1.2",
    "dynamic-class-list": "2.0.2",
    "native-fetch": "4.0.2",
    "pathe": "^1.1.0",
    "serialize-to-js": "3.1.2",
    "sort-css-media-queries": "2.1.1",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "commitlint": "17.6.1",
    "core-js": "3.30.1",
    "eslint": "8.38.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-xss": "0.1.12",
    "finalhandler": "1.2.0",
    "get-port-please": "3.0.1",
    "husky": "8.0.3",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.1",
    "nuxt": "2.16.3",
    "pinst": "3.0.0",
    "playwright": "1.32.3",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.5.0",
    "postcss-nesting": "11.2.2",
    "postcss-preset-env": "8.3.1",
    "serve-static": "1.15.0",
    "stylelint": "14.16.1",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-standard": "29.0.0",
    "vite": "^4.2.1",
    "vitest": "0.30.1",
    "vue-headings": "1.0.0",
    "vue-svg-loader": "0.16.0",
    "webpack-bundle-analyzer": "4.8.0",
    "wicg-inert": "3.1.2"
  }
}
